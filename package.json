    "blockcypher": "^0.2.0",
    "bootstrap": "^3.3.7",
    "bootstrap-hover-dropdown": "^2.2.1",
    "bs58check": "^2.0.2",
    "buffer": "^5.0.6",
    "chart.js": "^2.7.1",
    "coinkey": "^2.0.0",
    "crypto-js": "^3.1.9-1",
    "ecurve": "^1.0.5",
    "electron-builder-lib": "^20.13.2",
    "electron-context-menu": "^0.9.1",
    "electron-json-storage": "^3.0.7",
    "electron-save-file": "^1.0.2",
    "elliptic": "^6.4.0",
    "enzyme-adapter-react-15": "^1.0.1",
    "ethereum-address": "0.0.4",
    "ethereumjs-util": "^5.1.5",
    "fast-sha256": "^1.1.0",
    "file-loader": "^0.9.0",
    "fs": "0.0.1-security",
    "isomorphic-fetch": "^2.2.1",
    "jayson": "^2.0.1",
    "js-scrypt": "^0.2.0",
    "ledger-node-js-api": "git+https://github.com/coranos/ledger-node-js-api.git#1.1.5",
    "ledgerco": "^1.1.3",
    "lie": "^3.3.0",
    "lodash": "^4.17.4",
    "moment": "^2.19.1",
    "neon-js": "git+https://github.com/CityOfZion/neon-js.git#e1cc32b710cd71e0cf12b3172a0bade3af813c1b",
    "node-hid": "0.5.7",
    "node-loader": "^0.6.0",
    "numeral": "^2.0.6",
    "prop-types": "^15.6.0",
    "qrcode": "^0.8.2",
    "qrcode.react": "^0.7.2",
    "react": "^15.6.2",
    "react-ace": "^5.6.2",
    "react-bootstrap-modal": "^4.0.0",
    "react-chartjs-2": "^2.6.4",
    "react-countup": "^3.0.3",
    "react-dom": "^15.6.2",
    "react-hot-loader": "^3.0.0-beta.2",
    "react-icons": "^2.2.5",
    "react-modal": "^3.1.2",
    "react-redux": "^5.0.7",
