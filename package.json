{
  "name": "Morpheus-Beta",
  "version": "0.0.583",
  "license": "GPL-3.0",
  "main": "./main.js",
  "description": "Universal Wallet for NEO, BTC, LTC and ETH",
  "repository": {
    "type": "git",
    "url": "https://github.com/MorpheusWallet/beta.git"
  },
  "author": "Morpheus S.S. Ltd",
  "scripts": {
    "postinstall": "electron-rebuild",
    "electron-rebuild": "electron-rebuild",
    "start": "electron --enable-logging .",
    "start-mock": "SHAPESHIFT_API=mock electron .",
    "assets": "webpack",
    "assets-watch": "webpack --watch",
    "test": "jest",
    "test-watch": "jest --watch",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "dist-win": "build -w --ia32",
    "lint-fix": "eslint app/** --fix"
  },
  "keywords": [],
  "build": {
    "appId": "com.electron.morpheus",
    "files": [
      "./main.js",
      "dist/index.html",
      "dist/bundle.js",
      "dist/"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    }
  },
  "dependencies": {
    "@cityofzion/neon-js": "^3.7.0",
    "axios": "^0.17.0",
    "base-x": "^3.0.2",
    "bigi": "^1.4.2",
    "bitcoinjs-lib": "^3.3.2",
